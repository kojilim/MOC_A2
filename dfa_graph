	rankdir=LR size="8,5"
	"" [shape=circle style=invis]
	"" -> q0
	q0 [shape=circle]
	q1 [shape=circle]
	q2 [shape=circle]
	q3 [shape=circle]
	q4 [shape=circle]
	q5 [shape=doublecircle]
	q0 -> q0 [label=1]
	q0 -> q1 [label=0]
	q1 -> q0 [label=1]
	q1 -> q2 [label=0]
	q2 -> q1 [label=0]
	q2 -> q3 [label=1]
	q3 -> q3 [label=1]
	q3 -> q4 [label=0]
	q4 -> q3 [label=1]
	q4 -> q5 [label=0]
	q5 -> q5 [label=1]
	q5 -> q4 [label=0]
}
